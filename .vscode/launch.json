{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Driver Extension",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			],
			"preLaunchTask": "${defaultBuildTask}",
			"env": {
				"SQLTOOLS_DEBUG_PORT_LS": "6099"
			}
		}
	]
}